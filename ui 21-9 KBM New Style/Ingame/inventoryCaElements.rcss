tab.icon-tab {
    display: inline-block;
    decorator: none;
    color: tabs-default;
    pointer-events: auto;
    padding: 3dp;
    decorator: image(InventoryFilter-default contain);
}

tab.icon-tab:disabled {
    decorator: image(InventoryFilter-default contain);
    color: tabs-disabled;
}

tab.icon-tab:focus-visible,
tab.icon-tab:focus,
tab.icon-tab:hover {
    decorator: image(InventoryFilter-hover contain);
    color: tabs-hover;
}

tab.icon-tab:focus-visible:selected,
tab.icon-tab:focus:selected,
tab.icon-tab:hover:selected,
tab.icon-tab:selected {
    decorator: image(InventoryFilter-active contain);
    color: tabs-active;
}

