.tooltip-generic {
    pointer-events: none;
    width: 227dp;
    position: absolute;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: column;
}

.tooltip-wide
{
    pointer-events: none;
    width: 260dp;
    position: absolute;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: column;
}

.tooltip-frame,
.tooltip-gradient
{
    pointer-events: none;
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: column;
}







